			<nail x="-200" y="-288"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="-376" y="-440">mode == 4 || battery &lt; 10</label>
			<label kind="assignment" x="-312" y="-464">mode &lt;= 4</label>
			<nail x="-208" y="-440"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="guard" x="-464" y="-384">mode == 3</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
