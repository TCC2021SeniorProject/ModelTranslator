			<nail x="-608" y="-440"/>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id4"/>
			<label kind="guard" x="-872" y="-368">battery &gt; 10 &amp;&amp; mode == 1</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
r1 = Roomba_Test(status1, charge1);

// List one or more processes to be composed into a system.
system r1;</system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
